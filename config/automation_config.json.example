{
  "vps_provider": {
    "name": "digitalocean",
    "api_token": "YOUR_DO_API_TOKEN",
    "ssh_key_id": "YOUR_SSH_KEY_ID",
    "region": "fra1",
    "size": "s-2vcpu-4gb"
  },
  "dns_provider": {
    "name": "cloudflare",
    "api_token": "YOUR_CF_API_TOKEN",
